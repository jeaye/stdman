.SH Example

   
// Run this code

 #include <iostream>
 #include <iterator>
 #include <vector>

 void print(const std::vector<int>& vec)
 {
     std::cout << "[";
     for (auto& elem : vec)
         std::cout << " " << elem;
     std::cout << " ]\\n";
 }

 int main()
 {
     std::vector<int> vec;
     std::back_insert_iterator< std::vector<int> > it(vec);

     for (int i=0; i<10; ++i)
         it = i; // calls vec.push_back(i)

     print(vec);
 }

