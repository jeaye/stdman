.SH Example


// Run this code

 #include <iostream>
 #include <vector>

 int main()
 {
     // Create a vector containing integers
     std::vector<int> v = {8, 4, 5, 9};

     // Add two more integers to vector
     v.push_back(6);
     v.push_back(9);

     // Overwrite element at position 2
     v[2] = -1;

     // Print out the vector
     for (int n : v)
         std::cout << n << ' ';
     std::cout << '\\n';
 }

